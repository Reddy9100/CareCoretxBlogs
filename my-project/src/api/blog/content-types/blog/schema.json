{
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "subTitle": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "blocks",
      "required": true
    },
    "image": {
      "allowedTypes": ["images", "files", "videos", "audios"],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "publishedDate": {
      "type": "date",
      "required": true
    }
  }
}
